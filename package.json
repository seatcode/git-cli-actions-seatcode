{
  "name": "git-cli-actions-seatcode",
  "version": "1.0.0",
  "description": "Github actions to manage simple commands",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seatcode/git-cli-actions-seatcode.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/seatcode/git-cli-actions-seatcode/issues"
  },
  "homepage": "https://github.com/seatcode/git-cli-actions-seatcode#readme",
  "dependencies": {
    "@actions/artifact": "^1.1.0",
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@lhci/cli": "^0.9.0",
    "@lhci/utils": "^0.9.0",
    "is-windows": "^1.0.2",
    "lodash": "^4.17.21",
    "dayjs": "^1.8.23"
  },
  "devDependencies": {
    "@types/is-windows": "^1.0.0",
    "@types/lodash": "^4.14.188",
    "@types/node": "^18.11.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.0.1",
    "eslint-plugin-github": "^4.4.1",
    "eslint-plugin-jest": "^25.3.2",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.4.4"
  }
}
